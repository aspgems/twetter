<% setup(:title=>"People #{@duser.username} follows", :body_id=>"followers", :body_classes=>['friends'], :css=>['screen']) %>

<div id="followers">

    <h2><%= h @duser.name %> follows <%= @duser.friends_count %> people</h2>
    <br />

    <table class="followers-table doing" cellspacing="0">
          <%= render :partial=>'user/user', :collection=>@users %>
    </table>
</div>
